{# Voy a tener que generar asi cada form para poder customizar el atributo col-sm, que es diferente para cada
   form.
 #}
{{ form_start(form) }}
    {{ form_errors(form) }}
    <div class="form-group col-sm-8">
      {{ form_row(form.email) }}
    </div>
    <div class="form-group col-sm-8">
      {{ form_row(form.username) }}
    </div>
    <div class="form-group col-sm-8">
      {{ form_row(form.plainPassword) }}
    </div>
    <div class="form-group col-sm-8">
      {{ form_row(form.nombre) }}
    </div>
    <div class="form-group col-sm-8">
      {{ form_row(form.apellido) }}
    </div>
    <div class="form-group col-sm-8">
      {{ form_row(form.telefono) }}
    </div>
    <div class="form-group col-sm-8">
      {{ form_row(form.matricula) }}
    </div>
    <div class="form-group col-sm-8">
      <div class="col-sm-offset-6">
        {{ form_row(form.registrar) }}
      </div>
    </div>
{{ form_end(form) }}
